	<!-- Modal-Actualiza-tus datos-->
<div class="modal fade"
     id="actualizaD"
     tabindex="-1"
     aria-labelledby="exampleModalLabel2"
     aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered">
        <div class="modal-content p-5 bg-card border-rad-20">
            <div class="modal-header justify-close-btn border-0">
                <div class="button-container col-1 py-2 px-4">
                    <button type="button"
                            class="btn-close p-0"
                            data-bs-dismiss="modal"
                            aria-label="Close"></button>
                </div>
            </div>
            <form id="formActDatos" action="/" method="post">
                <div class="modal-body pt-0 px-4">
                    <h3 class="modal-title fs-2 fw-bold" id="exampleModalLabel">
                        Actualiza tus datos
                    </h3>
                    <p class="card-subtitle fs-5 fw-bold py-3">
                        A continuación completa tus datos:
                    </p>
                    <!-- Form -->
                    <div class="row">
                        <!-- Personal Data -->
                        <div class="col-md-6 col-12 my-2">
                            <div class="align-items-center justify-content-between w-100">
                                <label for="exampleFormControlInput1" class="form-label mb-0 px-3">RUT*</label>
                                <input type="text"
                                       class="form-control w-100 border-0 p-3 input-text border-rad-10"
                                       id="rutActDatos"
                                       name="rutActDatos"
                                       placeholder="12345678-9" />
                            </div>
                        </div>
                        <div class="col-md-6 col-12 my-2">
                            <div class="align-items-center justify-content-between w-100">
                                <label for="exampleFormControlInput1" class="form-label mb-0 px-3">Nombre*</label>
                                <input type="text"
                                       class="form-control w-100 border-0 p-3 input-text border-rad-10"
                                       id="nombreActDatos"
                                       name="nombreActDatos"
                                       placeholder="Pérez" />
                            </div>
                        </div>
                        <div class="col-md-6 col-12 my-2">
                            <div class="align-items-center justify-content-between w-100">
                                <label for="exampleFormControlInput1" class="form-label mb-0 px-3">Domicilo*</label>
                                <input type="text"
                                       class="form-control w-100 border-0 p-3 input-text border-rad-10"
                                       id="direccionActDatos"
                                       name="direccionActDatos"
                                       placeholder="Avenida Las delicias, número 24" />
                            </div>
                        </div>
                        <div class="col-md-6 col-12 my-2">
                            <div class="align-items-center justify-content-between w-100">
                                <label for="exampleFormControlInput1" class="form-label mb-0 px-3">Comuna*</label>
                                <input type="text"
                                       class="form-control w-100 border-0 p-3 input-text border-rad-10"
                                       id="comunaActDatos"
                                       name="comunaActDatos"
                                       placeholder="Santiago centro" />
                            </div>
                        </div>
                        <div class="col-md-6 col-12 my-2">
                            <div class="align-items-center justify-content-between w-100">
                                <label for="exampleFormControlInput1" class="form-label mb-0 px-3">Telefono Movil*</label>
                                <input type="text"
                                       class="form-control w-100 border-0 p-3 input-text border-rad-10"
                                       id="telMovilActDatos"
                                       name="telMovilActDatos"
                                       placeholder="+56 9 12345678" />
                            </div>
                        </div>
                        <div class="col-md-6 col-12 my-2">
                            <div class="align-items-center justify-content-between w-100">
                                <label for="exampleFormControlInput1" class="form-label mb-0 px-3">Telefono*</label>
                                <input type="text"
                                       class="form-control w-100 border-0 p-3 input-text border-rad-10"
                                       id="telefonoActDatos"
                                       name="telefonoActDatos"
                                       placeholder="2 32102315" />
                            </div>
                        </div>
                    </div>
                </div>
            </form>
            <div class="modal-footer border-0 p-4">
                <div class="w-100 d-flex justify-content-sm-end justify-content-center">
                    <div class="col-sm-5 col-12">
                        <button type="button"
                                onclick="GuardarActDatos()"
                                @*data-bs-toggle="modal"
                                data-bs-target="#datoslistos"*@
                                class="w-100 btn btn-danger bg-red standard-btn border-red hover-normal inverse text-uppercase d-block adjust-btn">
                            Enviar
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Modal-Dat0s-Actualizados -->
<div class="modal fade"
     id="datoslistos"
     tabindex="-1"
     aria-labelledby="exampleModalLabel"
     aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered">
        <div class="modal-content p-3 bg-card border-rad-20">
            <div class="modal-header justify-content-end no-align border-0">
                <div class="button-container col-2 py-2 d-flex justify-content-end">
                    <button type="button"
                            class="btn-close p-0"
                            data-bs-dismiss="modal"
                            aria-label="Close"></button>
                </div>
            </div>
            <div class="modal-body pt-0 px-4">
                <h3 class="modal-title fs-2 fw-bold text-center" id="exampleModalLabel">
                    Tus datos han sido actualizados correctamente
                </h3>
            </div>
            <div class="modal-footer border-0 p-4">
                <div class="w-100 d-flex justify-content-center">
                    <div class="col-sm-9 col-12">
                        <button type="button"
                                data-bs-dismiss="modal"
                                aria-label="Close"
                                class="w-100 btn btn-danger bg-red standard-btn border-red hover-normal inverse text-uppercase d-block adjust-btn">
                            <img class="left-arrow-icon"
                                 src="~/img/icons/left-white-arrow.svg"
                                 alt="" />
                            volver al home
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
